<h3>GAME</h3>
<% @width = "512" %>
<% @height = "512" %>

<div class="labyrinths">
  <div data-controller="main">
    <canvas id="myMove" width=<%= @width %> height=<%= @height %>></canvas>
    <canvas id="otherMove" width=<%= @width %> height=<%= @height %>></canvas>
    <%= button_to 'Left', '/main', method: :get %>
    <%= button_to 'Up', '/main', method: :get %>
    <%= button_to 'Right', '/main', method: :get %>
    <%= button_to 'Down', '/main', method: :get %>
  </div>
</div>
