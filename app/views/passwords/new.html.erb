<%= render("layouts/navbar") %>
<%= stylesheet_link_tag "form" %>
<div class="page">
  <div class="form">
    <%= form_with url: passwords_path, scope: :user do |form| %>
      <%= form.email_field :email, required: true, placeholder: "Enter Email" %>
      <%= form.submit "Reset Password" %>
    <% end %>
  </div>
</div>